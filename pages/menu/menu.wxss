/* pages/menu/menu.wxss */
.van-search__content{
  border-radius: 20px !important;
}
.container .mynav .active{
  color: #0d35bb;
}
.container{
  padding: 10px;
}
  
.mynav{
  width: 100%;
  height: 70px;
  display: flex;
  justify-content: center;
  text-align: center;
}
.item{
  width: 20%;
  height: 100%;
  line-height: 100%;
}
.item .image .images{
  width: 50px;
  height: 50px;
}
.text{
  font-size: 15px;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}